{
  "name": "@russ-ui/bibli",
  "version": "1.0.0",
  "description": "Библиотека общих UI компонентов, composables, API утилит и виджетов для Vue 3 проектов",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./shared/ui": {
      "import": "./dist/shared/ui/index.js",
      "types": "./dist/shared/ui/index.d.ts"
    },
    "./shared/theme": {
      "import": "./dist/shared/theme/index.js",
      "types": "./dist/shared/theme/index.d.ts"
    },
    "./widgets": {
      "import": "./dist/widgets/index.js",
      "types": "./dist/widgets/index.d.ts"
    }
  },
  "files": [
    "dist",
    "docs",
    "README.md"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vue",
    "vue3",
    "ui",
    "components",
    "library",
    "russ-ui"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "dependencies": {
    "@fancyapps/ui": "^5.0.0",
    "axios": "^1.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vue": "^3.4.0",
    "vue-router": "^4.2.0",
    "vue-tsc": "^1.8.0"
  }
}
